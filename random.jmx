          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">char correlated_values[100][50];  // Array to store up to 100 correlated values (50 characters max for each value)
int total_values = 10;             // To store the total number of captured values
int random_index;                 // To store the index of the randomly selected value



// Step 4: Store the captured values in an array
for (int i = 0; i &lt; total_values; i++) {
    // Save each captured value from the correlation parameter into the array
    sprintf(correlated_values[i], &quot;%s&quot;, lr_eval_string(lr_paramname(&quot;OrderIDs&quot;) + i));
}

// Step 5: Generate a random index to select a random value from the captured values
random_index = range(0, total_values - 1);  // Generate a random number between 0 and total_values-1

// Step 6: Print the randomly selected value
system.out.println(&quot;Randomly selected Order ID: %s\n&quot;, correlated_values[random_index]);

</stringProp>
